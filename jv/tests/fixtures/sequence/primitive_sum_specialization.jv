import jv.collections.*

fun main(): Unit {
    val characters = ['A' 'B' 'C']

    val charSum = characters.stream().sumIntFamily()
    println(charSum)

    val shorts = [5s 7s 9s]

    val shortSum = shorts.stream().sumIntFamily()
    println(shortSum)

    val numbers = [10 20 30]
    val intSum = numbers
        .map { value -> value }
        .sum()
    println(intSum)

    val longs = [100L 200L 300L]
    val longSum = longs.stream().sumLongFamily()
    println(longSum)
}
