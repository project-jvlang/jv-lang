package patterns;

public record Matrix(int row, int column);

public record Cell(Matrix matrix, int offset);

public class Example28 {
    public static int index(Cell cell) {
        return // strategy=Switch arms=2 guards=0 default=true exhaustive=true
        new Object() {
            int matchExpr() {
                final var __subject = cell;
                final int __matchResult;
                boolean __matched = false;
                if (!__matched) {
                    do {
                        if (!(__subject instanceof Cell(Matrix(var row, var column), var offset) c)) {
                            break;
                        }
                        __matched = true;
                        __matchResult = row * 100 + column + offset;
                        break;
                    } while (false);
                }
                if (!__matched) {
                    __matchResult = cell.offset();
                    __matched = true;
                }
                return __matchResult;
            }
        }.matchExpr()
        ;
    }
    
}
