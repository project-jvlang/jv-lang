package patterns;

public class Example13 {
    public static String label(int score) {
        return // strategy=Switch arms=4 guards=3 default=true exhaustive=false
        new Object() {
            String matchExpr() {
                final var __subject = score;
                final String __matchResult;
                boolean __matched = false;
                if (!__matched) {
                    if ((__subject instanceof int it0) && (score >= 10 && score < 30)) {
                        __matchResult = "low";
                        __matched = true;
                    }
                }
                if (!__matched) {
                    if ((__subject instanceof int it0) && (score >= 30 && score <= 60)) {
                        __matchResult = "mid";
                        __matched = true;
                    }
                }
                if (!__matched) {
                    if ((__subject instanceof int it0) && (score >= 60 && score < 90)) {
                        __matchResult = "upper";
                        __matched = true;
                    }
                }
                if (!__matched) {
                    __matchResult = "extreme";
                    __matched = true;
                }
                return __matchResult;
            }
        }.matchExpr()
        ;
    }
    
}
