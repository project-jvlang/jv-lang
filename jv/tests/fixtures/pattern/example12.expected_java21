package patterns;

public record Payload(int left, int right);

public class Example12 {
    public static int aggregate(Object value) {
        return // strategy=Switch arms=4 guards=2 default=true exhaustive=false
        new Object() {
            int matchExpr() {
                final var __subject = value;
                final int __matchResult;
                boolean __matched = false;
                if (!__matched) {
                    do {
                        if (!(__subject instanceof String __tmp0)) {
                            break;
                        }
                        if (!(__tmp0.contains(" "))) {
                            break;
                        }
                        __matched = true;
                        __matchResult = __tmp0.length() * 2;
                        break;
                    } while (false);
                }
                if (!__matched) {
                    do {
                        if (!(__subject instanceof Integer __tmp1)) {
                            break;
                        }
                        if (!(__tmp1 % 3 == 0)) {
                            break;
                        }
                        __matched = true;
                        __matchResult = __tmp1 / 3;
                        break;
                    } while (false);
                }
                if (!__matched) {
                    do {
                        if (!(__subject instanceof Payload(var left, var right) payload)) {
                            break;
                        }
                        __matched = true;
                        __matchResult = left + right;
                        break;
                    } while (false);
                }
                if (!__matched) {
                    __matchResult = 1;
                    __matched = true;
                }
                return __matchResult;
            }
        }.matchExpr()
        ;
    }
    
}
