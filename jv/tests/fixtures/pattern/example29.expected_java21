package patterns;

public record Bounds(int min, int max);

public record Window(Bounds bounds, int padding);

public class Example29 {
    public static int size(Window window) {
        return // strategy=Switch arms=2 guards=0 default=true exhaustive=true
        new Object() {
            int matchExpr() {
                final var __subject = window;
                final int __matchResult;
                boolean __matched = false;
                if (!__matched) {
                    do {
                        if (!(__subject instanceof Window(Bounds(var min, var max), var padding) w)) {
                            break;
                        }
                        __matched = true;
                        __matchResult = (max - min) + padding * 2;
                        break;
                    } while (false);
                }
                if (!__matched) {
                    __matchResult = window.padding();
                    __matched = true;
                }
                return __matchResult;
            }
        }.matchExpr()
        ;
    }
    
}
