package patterns;

public record Range(int start, int end);

public record Span(Range range, int bias);

public class Example26 {
    public static int spread(Span span) {
        return // strategy=Switch arms=2 guards=0 default=true exhaustive=true
        new Object() {
            int matchExpr() {
                final var __subject = span;
                final int __matchResult;
                boolean __matched = false;
                if (!__matched) {
                    do {
                        if (!(__subject instanceof Span(Range(var start, var end), var bias) s)) {
                            break;
                        }
                        __matched = true;
                        __matchResult = end - start + bias;
                        break;
                    } while (false);
                }
                if (!__matched) {
                    __matchResult = span.bias();
                    __matched = true;
                }
                return __matchResult;
            }
        }.matchExpr()
        ;
    }
    
}
