package patterns;

public record Vector(int x, int y);

public record Point(Vector vector, int weight);

public class Example23 {
    public static int magnitude(Point value) {
        return // strategy=Switch arms=2 guards=0 default=true exhaustive=true
        new Object() {
            int matchExpr() {
                final var __subject = value;
                final int __matchResult;
                boolean __matched = false;
                if (!__matched) {
                    do {
                        if (!(__subject instanceof Point(Vector(var x, var y), var weight) point)) {
                            break;
                        }
                        __matched = true;
                        __matchResult = x * x + y * y + weight;
                        break;
                    } while (false);
                }
                if (!__matched) {
                    __matchResult = value.weight();
                    __matched = true;
                }
                return __matchResult;
            }
        }.matchExpr()
        ;
    }
    
}
