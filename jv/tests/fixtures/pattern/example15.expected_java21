package patterns;

public class Example15 {
    public static String stage(int score) {
        return // strategy=Switch arms=4 guards=3 default=true exhaustive=false
        new Object() {
            String matchExpr() {
                final var __subject = score;
                final String __matchResult;
                boolean __matched = false;
                if (!__matched) {
                    if ((__subject instanceof int it0) && (score >= 0 && score < 25)) {
                        __matchResult = "alpha";
                        __matched = true;
                    }
                }
                if (!__matched) {
                    if ((__subject instanceof int it0) && (score >= 25 && score < 50)) {
                        __matchResult = "beta";
                        __matched = true;
                    }
                }
                if (!__matched) {
                    if ((__subject instanceof int it0) && (score >= 50 && score <= 75)) {
                        __matchResult = "gamma";
                        __matched = true;
                    }
                }
                if (!__matched) {
                    __matchResult = "omega";
                    __matched = true;
                }
                return __matchResult;
            }
        }.matchExpr()
        ;
    }
    
}
