data class Range(val start: Int, val end: Int)
data class Span(val range: Range, val bias: Int)

fun spread(span: Span): Int {
    return when (span) {
        Span(Range(start, end), bias) -> end - start + bias
        else -> bias
    }
}

fun main() {
    val input = Span(Range(2, 9), 3)
    println(spread(input))
}
