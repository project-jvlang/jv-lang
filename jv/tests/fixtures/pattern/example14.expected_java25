package patterns;

public class Example14 {
    public static String classify(int score) {
        return // strategy=Switch arms=4 guards=3 default=true exhaustive=false
        switch (score) {
            // range: -100..-10
            case int it0 when (score >= -100 && score < -10) -> "very-low"
            // range: -10..10
            case int it0 when (score >= -10 && score < 10) -> "near-zero"
            // range: 10..=40
            case int it0 when (score >= 10 && score <= 40) -> "positive"
            default -> "wild"
        }
        ;
    }
    
}
